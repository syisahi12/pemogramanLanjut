@model IEnumerable<WebAppRating.Models.Article>
@{
    if (Session["UserId"] == null)
    {
        Response.Redirect("~/Admin/Index");
    }
    ViewBag.Title = "Index";
    ViewBag.LoginIcon = "glyphicon glyphicon-log-out";
    ViewBag.LoginTitle = "LogOut";
    ViewBag.LoginUrl = Url.Action("LogOut","Admin");
}
<p style="margin-top: 100px">
    @if (@Session["userName"] != null)
    {
        <h2>Welcome back, @Session["userName"].ToString()</h2>
    }
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ArticleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ArticleDescription)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ArticleName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ArticleDescription)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ArticleId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ArticleId })
            </td>
        </tr>
    }

</table>